{
  "name": "ship",
  "version": "1.0.0",
  "description": "Ship - Autonomous AI Coding Agent that works with you",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ship": "./bin/ship.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ai": {
      "types": "./dist/lib/ai/index.d.ts",
      "import": "./dist/lib/ai/index.js"
    },
    "./agents": {
      "types": "./dist/agents/orchestrator.d.ts",
      "import": "./dist/agents/orchestrator.js"
    },
    "./mcp": {
      "types": "./dist/mcp/server.d.ts",
      "import": "./dist/mcp/server.js"
    }
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "npm run build:app && npm run build:cli",
    "build:app": "next build",
    "build:cli": "tsc -p tsconfig.cli.json",
    "start": "next start",
    "lint": "next lint && eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "ship": "tsx src/cli/index.ts",
    "mcp": "tsx src/mcp/server.ts",
    "prepare": "npm run build:cli"
  },
  "keywords": [
    "ai",
    "coding-agent",
    "mcp",
    "github",
    "automation",
    "llm",
    "code-review",
    "pull-request",
    "issue-tracker"
  ],
  "author": "Ship Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/likhonsex/Ship.git"
  },
  "bugs": {
    "url": "https://github.com/likhonsex/Ship/issues"
  },
  "homepage": "https://github.com/likhonsex/Ship#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ai-sdk/groq": "^2.0.32",
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/xai": "^1.0.0",
    "@ai-sdk/react": "^2.0.109",
    "@e2b/code-interpreter": "^2.3.3",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@octokit/rest": "^21.0.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@vercel/speed-insights": "^1.0.0",
    "ai": "^5.0.108",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "commander": "^12.0.0",
    "firebase": "^11.0.0",
    "geist": "^1.3.0",
    "lucide-react": "^0.460.0",
    "next": "^14.2.18",
    "next-themes": "^0.4.0",
    "ora": "^8.0.0",
    "p-retry": "^6.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "simple-git": "^3.25.0",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.18",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "ollama-ai-provider": "^0.16.0"
  },
  "peerDependenciesMeta": {
    "ollama-ai-provider": {
      "optional": true
    }
  }
}
